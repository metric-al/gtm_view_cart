___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const copyFromWindow = require('copyFromWindow');
const callInWindow = require('callInWindow');

const payload = {
  cart: null
};

if (data.cart) {
  payload.cart = data.cart.map((cartObj) => {
    return {
      id: cartObj[data.cartObjectIdVarName],
      quantity: cartObj[data.cartObjectQuantityVarName],
      price: cartObj[data.cartObjectPriceVarName],
      sku: cartObj[data.cartObjectSkuVarName]
    };
  });
}

const viewCartTopic = copyFromWindow('_MetricalAbandonCart.EventBus.constants.topics.ACTION_VIEW_CART');
const viewCartPublish = () => callInWindow('_MetricalAbandonCart.EventBus.publish', viewCartTopic, payload);

callInWindow('_MetricalTopicPublishBuffer.push', viewCartPublish);

data.gtmOnSuccess();


